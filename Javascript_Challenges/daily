1. For(var i = 1; i <= 10; i++){
	console.log(i);
}
2. var firstName =' Markus';
var lastName =' Beamer';
console.log(' hello there! My name is ' + firstName + lastName);
3. for(i = 0; i <= 4; i++){
  console.log('Alex Beamer');
}
4. var names = ['Alex', 'Nick', 'Markus', 'Sherry'];

for (var i = 0; i < names.length; i++){
  console.log(names[i]);
}
5. for(var i=10; i >=1; i--){
  console.log(i)
}
